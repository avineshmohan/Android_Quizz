<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.octagon.root.onlinequizz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon1"
        android:supportsRtl="true"
        android:noHistory="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".BroadcastReceiver.AlarmReceiver" />

        <activity android:name=".Home" />
        <activity android:name=".Start" />
        <activity android:name=".Begining" />
        <activity android:name=".Done" />
        <activity android:name=".ScoreDetail" />
        <activity android:name=".Signin" />
        <activity android:name=".Signup"></activity>
    </application>

</manifest>